<%- include ('../partials/header') %>

    <main>
        <h1>Login do Aluno</h1>
        <h2>Insira os seus dados</h2>
    <form action="/logar-aluno" method="post">
        <label for="email">Email:</label>
        <input type="email" id="email" name="email" placeholder="Email do Aluno">
        <br>
        <Label for="senha">Senha:</Label>
        <input type="password" name="senha" id="senha" placeholder="Senha do Aluno">
        <input type="submit" value="Logar">
        <br><br>
        <button type="button"><a href="/cadastro-aluno">Voltar</a></button>
    </main>

<%- include ('../partials/footer') %>